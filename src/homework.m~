% [LC]=size(imgg);
% L -> 512
% C -> 512

% i=15

clear all

imgg = imread('database/lena512.bmp');

N = 15;

for i = 1 : N
    imageN = noise(imgg);
    image(:,:,i) = imageN;
end

figure, imshow(image);